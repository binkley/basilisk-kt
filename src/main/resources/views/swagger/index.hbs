<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Swagger-ui</title>

    <link rel="icon" type="image/png"
          href="//unpkg.com/swagger-ui-dist@{{version}}/favicon-32x32.png">
    <link rel="stylesheet" type="text/css"
          href="//unpkg.com/swagger-ui-dist@{{version}}/swagger-ui.css">

    <script src="//unpkg.com/swagger-ui-dist@{{version}}/swagger-ui-standalone-preset.js"></script>
    <script src="//unpkg.com/swagger-ui-dist@{{version}}/swagger-ui-bundle.js"></script>
</head>
<body>
<div id="swagger-ui"></div>
<script>
    window.onload = function () {
        window.ui = SwaggerUIBundle({
            urls: [{{#each urls}}
                {
                    name: "{{name}}",
                    url: "{{url}}"
                }{{#unless @last}},{{/unless}}{{/each}}
            ],
            dom_id: '#swagger-ui',
            deepLinking: {{deepLinking}},
            displayRequestDuration: {{displayRequestDuration}},
            showCommonExtensions: {{showCommonExtensions}},
            showExtensions: {{showExtensions}},
            presets: [
                SwaggerUIBundle.presets.apis,
                SwaggerUIStandalonePreset
            ],
            plugins: [
                SwaggerUIBundle.plugins.DownloadUrl
            ],
            layout: "{{layout}}"
        });
    }
</script>
</body>
</html>
